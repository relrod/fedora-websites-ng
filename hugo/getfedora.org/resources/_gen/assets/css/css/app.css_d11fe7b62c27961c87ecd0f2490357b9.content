:root{--fedora-blue: #3c6eb4;--fedora-dark-blue: #294172;--fedora-magenta: #db3279;--fedora-orange: #e59728;--fedora-green: #79db32;--fedora-purple: #a07cbc}body,.tooltip,.popover{font-family:open sans,-apple-system,BlinkMacSystemFont,segoe ui,Roboto,helvetica neue,Arial,sans-serif,apple color emoji,segoe ui emoji,segoe ui symbol,noto color emoji}.fedora-dotted-background{position:absolute;width:100%;height:2000px;top:-40px;z-index:-100;background:url(../images/dotted-fedora-background.png) #fff;background-repeat:no-repeat;background-position:100% 0}.editions-cutout-green{background:url(../images/green-corner-cutout.png) #fff;background-repeat:no-repeat;background-position:100% 100%;background-size:100px}.editions-cutout-orange{background:url(../images/orange-corner-cutout.png) #fff;background-repeat:no-repeat;background-position:100% 100%;background-size:100px}.editions-cutout-purple{background:url(../images/purple-corner-cutout.png) #fff;background-repeat:no-repeat;background-position:100% 100%;background-size:100px}.editions-cutout-magenta{background:url(../images/magenta-corner-cutout.png) #fff;background-repeat:no-repeat;background-position:100% 100%;background-size:100px}.bg-gradient-tranparent-to-003{background:linear-gradient(transparent,rgba(0,0,0,0.03))}.bg-gradient-light{background:linear-gradient(white,#e3e3e3)}.bg-gradient-dark{background:linear-gradient(rgb(68,68,68),#202020)}.bg-fedora-blue{background:var(--fedora-blue)}.bg-fedora-darkblue{background:var(--fedora-darkblue)}.bg-fedora-magenta{background:var(--fedora-magenta)}.bg-fedora-orange{background:var(--fedora-orange)}.bg-fedora-green{background:var(--fedora-green)}.bg-fedora-purple{background:var(--fedora-purple)}.rounded-left-1{border-top-left-radius:1rem;border-bottom-left-radius:1rem}.mt--1{margin-top:-1rem}.mt--2{margin-top:-2rem}.mt--3{margin-top:-3rem}.line-height-1{line-height:1rem}.absolute-bottom{position:absolute!important;bottom:0!important}a.text-white:hover{color:#fff!important;text-shadow:1px 1px 0 #ccc,2px 2px 0 #999}